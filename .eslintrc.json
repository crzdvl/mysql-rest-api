{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base/legacy",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": [
            0,
            0
        ],
        "strict": 0,
        "no-var": "error",
        "no-console": 0,
        "no-fallthrough": 0,
        "consistent-return": 0,
        "max-len": 0,
        "func-names": 0,
        "no-await-in-loop": 0,
        "no-restricted-syntax": 0,
        "camelcase": 0
    }
}
